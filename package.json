{
  "name": "bedroom.projects-proto-2",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && NODE_ENV=production APP_ENV=production vite build",
    "serve": "vite preview --port 3000",
    "postinstall": "npx patch-package",
    "typecheck": "tsc --noEmit",
    "tc": "npm run typecheck",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "lint:style": "stylelint --config .stylelintrc src/{styles/*.ts,**/styled.ts,**/styled/index.ts}",
    "test": "npm run tc && npm run lint"
  },
  "dependencies": {
    "@welldone-software/why-did-you-render": "6.2.3",
    "body-scroll-lock": "3.1.5",
    "immer": "9.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "6.1.0",
    "react-hotjar": "4.0.0",
    "react-location": "3.2.15",
    "react-location-elements-to-routes": "3.2.15",
    "showdown": "1.9.1",
    "styled-components": "5.3.3",
    "zustand": "3.6.5"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@types/body-scroll-lock": "3.1.0",
    "@types/node": "16.11.11",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "6.1.4",
    "@types/resize-observer-browser": "0.1.6",
    "@types/showdown": "1.9.4",
    "@types/styled-components": "5.1.16",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "stylelint": "14.1.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-html": "2.1.1",
    "vite-plugin-react-svg": "0.2.0",
    "vite-tsconfig-paths": "3.3.17"
  }
